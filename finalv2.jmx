            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Select Product" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper

def jsonResponse = new JsonSlurper().parseText(prev.getResponseDataAsString())

def items = jsonResponse.Items

def randomIndex = new Random().nextInt(items.size())
def selectedItem = items[randomIndex]

vars.put(&quot;selected_id&quot;, selectedItem.id.toString())
vars.put(&quot;selected_cat&quot;, selectedItem.cat)
vars.put(&quot;selected_desc&quot;, selectedItem.desc)
vars.put(&quot;selected_img&quot;, selectedItem.img)
vars.put(&quot;selected_price&quot;, selectedItem.price.toString())
vars.put(&quot;selected_title&quot;, selectedItem.title)

log.info(&quot;Randomly selected item: ID=${selectedItem.id}, Title=${selectedItem.title}&quot;)</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
            </JSR223PostProcessor>
            <hashTree/>
            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Generate Random UUID" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def randomString = { length, chars -&gt;
    def random = new Random()
    def randomChars = new StringBuilder()
    length.times {
        def randomIndex = random.nextInt(chars.length())
        randomChars.append(chars.charAt(randomIndex))
    }
    return randomChars.toString()
}

def allowedChars = &quot;abcdef0123456789&quot;

def customUUID = randomString(8, allowedChars) + &quot;-&quot; + 
                randomString(4, allowedChars) + &quot;-&quot; + 
                randomString(4, allowedChars) + &quot;-&quot; + 
                randomString(8, allowedChars)

vars.put(&quot;custom_uuid&quot;, customUUID)</stringProp>
            </JSR223PostProcessor>
